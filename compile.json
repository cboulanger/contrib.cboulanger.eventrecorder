{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "environment": {
    "qx.icontheme": ["Tango", "Oxygen"],
    "qx.debug": true,
    "module.objectid": true
  },
  "applications": [
    {
      "class": "cboulanger.eventrecorder.demo.Application",
      "theme": "cboulanger.eventrecorder.theme.Theme",
      "name": "eventrecorder",
      "title": "Simple event recorder demo"
    },
    {
      "class": "qxl.widgetbrowser.Application",
      "theme": "qx.theme.Indigo",
      "name": "widgetbrowser_tooltip",
      "title": "WidgetBrowser with object id tooltip",
      "include": [
        "qx.theme.Modern",
        "qx.theme.Simple",
        "qx.theme.Classic",
        "qxl.versionlabel.VersionLabel",
        "qxl.formdemo.FormItems",
        "cboulanger.eventrecorder.ObjectIdGenerator",
        "cboulanger.eventrecorder.ObjectIdTooltip"
      ],
      "environment": {
        "qx.icontheme": ["Tango", "Oxygen"]
      }
    },
    {
      "class": "qxl.widgetbrowser.Application",
      "theme": "qx.theme.Indigo",
      "name": "widgetbrowser_recorder",
      "title": "WidgetBrowser with event recorder",
      "include": [
        "qx.theme.Modern",
        "qx.theme.Simple",
        "qx.theme.Classic",
        "qxl.versionlabel.VersionLabel",
        "qxl.formdemo.FormItems",
        "cboulanger.eventrecorder.UiController",
        "cboulanger.eventrecorder.type.Qooxdoo",
        "cboulanger.eventrecorder.ObjectIdGenerator"
      ]
    },
    {
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "title": "API Viewer",
      "environment": {
        "excludeFromAPIViewer": [
          "qxl.apiviewer.*"
        ]
      },
      "include": [
        "qx.*"
      ],
      "exclude": [
        "qx.test.*",
        "qx.module.Blocker",
        "qx.module.Placement"
      ]
    }
  ]
}